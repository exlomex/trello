{
  "name": "trello",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:withJsonServer": "concurrently \"npm run start-dev-server\" \"npm run dev\"",
    "build": "next build",
    "start": "next start",
    "lint:ts": "next lint",
    "lint:ts:fix": "next lint --fix",
    "unit": "jest --config ./jest.config.js",
    "lint:scss": "npx stylelint **/*.scss",
    "lint:scss:fix": "npx stylelint **/*.scss --fix",
    "prettier:fix": "prettier . --write",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "start-dev-server": "json-server json-server/db.json --port 5000"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.3",
    "@storybook/addon-themes": "^8.0.8",
    "concurrently": "^8.2.2",
